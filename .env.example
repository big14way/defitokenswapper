# Frontend Environment Variables (.env.local)
# WalletConnect Project ID (Get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Deployed TokenSwapper Contract Address
NEXT_PUBLIC_TOKEN_SWAPPER_ADDRESS=your_deployed_contract_address_here

# RPC URLs (Optional - chains have built-in defaults)
NEXT_PUBLIC_INFURA_KEY=your_infura_key_here
NEXT_PUBLIC_ALCHEMY_KEY=your_alchemy_key_here
